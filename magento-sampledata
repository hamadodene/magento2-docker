#!/bin/bash

CONTAINER_NAME = $1


docker exec  -u daemon -w /bitnami/magento -i $CONTAINER_NAME php bin/magento setup:upgrade
docker exec  -u daemon  -w /bitnami/magento -i $CONTAINER_NAME php bin/magento setup:static-content:deploy -f
docker exec  -u daemon  -w /bitnami/magento  -i $CONTAINER_NAME php bin/magento setup:di:compile
docker exec  -u daemon  -w /bitnami/magento  -i $CONTAINER_NAME php bin/magento cache:flush
docker exec  -u daemon  -w /bitnami/magento  -i $CONTAINER_NAME php bin/magento indexer:reindex 

 
